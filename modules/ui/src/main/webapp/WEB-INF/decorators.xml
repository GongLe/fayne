<?xml version="1.0" encoding="UTF-8"?>
<decorators defaultdir="/WEB-INF/layouts/">
	<excludes>
		<pattern>/static/*</pattern>
		<pattern>/404.jsp*</pattern>
	</excludes>
	
	<decorator name="default" page="default.jsp">
		<pattern>/*</pattern>
	</decorator>
    <decorator name="mooncake" page="mooncake.jsp">
		<pattern>/mooncake**</pattern>
	</decorator>
   <decorator name="fayne-ui" page="fayne-ui.jsp">
		<pattern>/fayne-ui**</pattern>
	</decorator>
 <decorator name="ace" page="ace.jsp">
		<pattern>/ace**</pattern>
	</decorator>

</decorators>
